using System;
using Person = ConsoleApp32.User;
using Printer = System.Console;
namespace ConsoleApp32
{
    class Program
    {
        static void Main(string[] args)
        {
            Person person = new Person();
            person.name = "Tom";
            Printer.WriteLine(person.name);
            Printer.ReadLine();
        }
    }
    class User
    {
        public string name;
    }
}
