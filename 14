using System;

namespace ConsoleApp14
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = 3;
            int y = 2;
            Console.WriteLine("Нажмите +  или -");
            string selection = Console.ReadLine();

            int z = selection == "+" ? (x + y) : (x - y);
            Console.WriteLine(z);
        }
    }
}
