using System;

namespace ConsoleApp28
{
    struct User
    {
        public string name;
        public int age;

        public void Display()
        {
            Console.WriteLine($"Name:{name} Age:{age}");
        }
    }
    class Program
    {
        static void Main(string[] args)
        {
            User tom;
            tom.name = "Tom";
            tom.age = 34;
            tom.Display();
            Console.ReadLine();
        }
    }
}
